#include <stdio.h>

int main() {
    int i,num,sumeven=0,sumodd=0;
    printf("enetr value of any number");
    scanf("%d",&num);
    for(i=1;i<=num;i=i+1){
        if(i%2==0)
        sumeven=sumeven+i;
        else
        sumodd = sumodd+i;
    }
    printf("sumeven=%d\n",sumeven);
    printf("sumodd=%d\n",sumodd);
    return 0;
}
        
    
