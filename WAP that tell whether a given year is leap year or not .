#include <stdio.h>
#include <math.h>

int main()
{
    int year;
    printf("enter a year");
    scanf("%d",&year);
    //leap year conditions//
    if(year % 4 == 0 || year % 400 ==0 & (year % 100!=0)){
        printf("given year is a leap year:%d\n",year);
    }
    else
    {
        printf("given year is not a leap year: %d\n",year);
    }
    return 0;
}
